    "@opentelemetry/sdk-trace-base": "^1.10.0",
    "@opentelemetry/semantic-conventions": "^1.9.1",
    "@opentelemetry/tracing": "^0.24.0",
    "aws-sdk": "^2.1335.0",
    "axios": "^1.3.4",
    "mysql2": "^3.2.0",
    "opentelemetry-instrumentation-aws-sdk": "^0.26.0",
